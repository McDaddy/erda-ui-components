  "sideEffects": [
    "dist/*",
    "es/*.js",
    "lib/*.js",
    "components/*.ts",
    "*.less",
    "**/*/style/index.js"
  ],
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "keywords": [
    "antd",
    "react",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
